    { TMVAssert(FALSE); }\
template <> void Q_RDivEq(const GenMatrix<T1 >& ,\
    const GenVector<T1 >& , const MatrixView<T >& )\
    { TMVAssert(FALSE); }\

#ifdef INST_COMPLEX
DefDivEq(T,CT)
#endif

#undef DefDivEq

#define DefDiv(T,T1,T2)\
template <> void QR_LDiv(const GenMatrix<T1 >& ,\
