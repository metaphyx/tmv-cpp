    const GenVector<T1 >& beta, const int* P, \
    const GenMatrix<T2 >& m, const MatrixView<T >& x, int N1); \

DefDiv(T,T,T)
#ifdef INST_COMPLEX
DefDiv(CT,T,T)
DefDiv(CT,T,CT)
DefDiv(CT,CT,T)
DefDiv(CT,CT,CT)
#endif

#undef DefDiv

#define DefDivEq(T,T1)\
template <> void QR_LDivEq(const GenMatrix<T1 >& ,\
    const GenVector<T1 >& , const int* , \
    const MatrixView<T >& , int )\
    { TMVAssert(FALSE); }\
template <> void QR_RDivEq(const GenMatrix<T1 >& ,\
