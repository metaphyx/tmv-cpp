
#define CT std::complex<T>

template class BandSVDiv<T>;
#ifdef INST_COMPLEX
template class BandSVDiv<CT >;
#endif

#define DefDivEq(T,T2)\
template void BandSVDiv<T >::DoLDivEq2(const MatrixView<T2 >& m) const; \
template void BandSVDiv<T >::DoRDivEq2(const MatrixView<T2 >& m) const; \
template void BandSVDiv<T >::DoInverse2(const MatrixView<T2 >& m) const; \

DefDivEq(T,T)
#ifdef INST_COMPLEX
DefDivEq(T,CT)
DefDivEq(CT,CT)

DefDivEq(CT,T)
#endif

#undef DefDivEq

#define DefDiv(T,T1,T2) \
template void BandSVDiv<T >::DoLDiv2(const GenMatrix<T1 >& m1, \
    const MatrixView<T2 >& m2) const; \
template void BandSVDiv<T >::DoRDiv2(const GenMatrix<T1 >& m1, \
    const MatrixView<T2 >& m2) const; \

DefDiv(T,T,T)
#ifdef INST_COMPLEX
DefDiv(T,T,CT)
DefDiv(T,CT,CT)
DefDiv(CT,T,CT)
DefDiv(CT,CT,CT)

DefDiv(CT,T,T)
DefDiv(CT,CT,T)
DefDiv(T,CT,T)
#endif

#undef DefDiv

#undef CT
