
#define CT std::complex<T>

template class HermCHDiv<T >;
#ifdef INST_COMPLEX
template class HermCHDiv<CT >;
#endif

#define DefDivEq(T,T2)\
template void HermCH_LDivEq(const GenSymMatrix<T >& LL,\
    const MatrixView<T2 >& b); \
template void HermCH_RDivEq(const GenSymMatrix<T >& LL,\
    const MatrixView<T2 >& b); \
template void HermCHDiv<T >::DoLDivEq(const MatrixView<T2 >& m) const; \
template void HermCHDiv<T >::DoRDivEq(const MatrixView<T2 >& m) const; \
template void HermCHDiv<T >::DoInverse(const MatrixView<T2 >& m) const; \
template void HermCHDiv<T >::DoInverse(const SymMatrixView<T2 >& m) const; \

DefDivEq(T,T)
#ifdef INST_COMPLEX
DefDivEq(T,CT)
DefDivEq(CT,CT)

DefDivEq(CT,T)
#endif

#undef DefDivEq

#define DefDiv(T,T1,T2) \
template void HermCHDiv<T >::DoLDiv(const GenMatrix<T1 >& m1, \
    const MatrixView<T2 >& m2) const; \
template void HermCHDiv<T >::DoRDiv(const GenMatrix<T1 >& m1, \
    const MatrixView<T2 >& m2) const; \

DefDiv(T,T,T)
#ifdef INST_COMPLEX
DefDiv(T,T,CT)
DefDiv(T,CT,CT)
DefDiv(CT,T,CT)
DefDiv(CT,CT,CT)

DefDiv(CT,T,T)
DefDiv(CT,CT,T)
DefDiv(T,CT,T)
#endif

#undef DefDiv

#undef CT



