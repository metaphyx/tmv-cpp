	  cat $*.d >> $(EXDIR)/.$*.P; \
	  sed -e 's/#.*//' -e 's/^[^:]*: *//' -e 's/ *\\$$//' \
	      -e '/^$$/ d' -e 's/$$/ :/' < $*.d >> $(EXDIR)/.$*.P; \
	  $(RM) $*.d

